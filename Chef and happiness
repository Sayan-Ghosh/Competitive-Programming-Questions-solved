#include <bits/stdc++.h> 
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
    int n,f=0;
    cin>>n;
    int a[n];
    set<int>s;
    for(int i=1;i<=n;i++)
    {
        cin>>a[i];
        s.insert(a[i]);
    }
    set<int> ss;
    for(auto it = s.begin();it!=s.end();it++)
    {
    if(ss.find(a[a[*it]])!=ss.end())
    {
        f=1;
        break;
    }
    ss.insert(a[a[*it]]);
    }
    if(f)cout<<"Truly Happy\n";
    else cout<<"Poor Chef\n";
    }
	return 0;
}
